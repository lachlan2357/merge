{
  "name": "merge",
  "version": "0.0.0",
  "description": "A Quality Assurance tool for OpenStreetMap contributors",
  "author": {
    "name": "Lachlan Rehder",
    "email": "lachlan.rehder@outlook.com",
    "url": "https://lachlan2357.github.io/"
  },
  "license": "GPL-3.0-only",
  "private": true,
  "type": "module",
  "scripts": {
    "check-ts": "tsc --noEmit",
    "lint": "eslint ./src --max-warnings 0",
    "knip": "knip",
    "format": "prettier --write ./src/",
    "doc": "typedoc",
    "check-all": "yarn check-ts && yarn lint && yarn knip && yarn format && yarn doc",
    "generate-icons": "realfavicon generate ./assets/merge.png favicon-settings.json favicon-output.json ./assets/icons",
    "merge": "node --experimental-strip-types ./cli"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^22.19.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "globals": "^16.5.0",
    "knip": "^5.72.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "prettier-plugin-jsdoc": "^1.8.0",
    "realfavicon": "^0.5.0",
    "sass": "^1.95.0",
    "serve": "^14.2.5",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.13.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "overrides": {
    "globals": "^15.14.0"
  },
  "packageManager": "yarn@4.6.0"
}
